
jQuery(document).ready (function () {
	angular.module ('SkyDataApp')
		.config ( function ($routeProvider) {
		{% for page in pages %}
			$routeProvider.when ('/{{ page.route }}', 
			{
				templateUrl: '{{ page.route }}',
				controller: '{{ page.class }}Controller'
			});
		{% endfor %}	
		});
});